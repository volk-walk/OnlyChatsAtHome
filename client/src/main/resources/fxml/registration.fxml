<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.ListView?>
<VBox fx:controller="client.RegistrationController"
      xmlns:fx="http://javafx.com/fxml" alignment="center">
    <stylesheets>
        <URL value="@/css/style.css"/>
    </stylesheets>

    <TextField fx:id="loginField"  promptText="Логин"/>
    <TextField fx:id="nicknameField"  promptText="Никнейм"/>
    <PasswordField fx:id="passwordField"  promptText="Пароль"/>
    <TextArea fx:id="textArea" VBox.vgrow="ALWAYS"/>
    <Button text="Зарегистрироваться" onAction="#tryToReg" minWidth="100"/>

</VBox>